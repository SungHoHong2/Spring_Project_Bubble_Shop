<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
	<script type="text/javascript">
		
		function reply_go() {
			document.frm.action = "replyOk";
			document.frm.submit();
		}
		
	</script>
	
</head>
<body>
	<form name="frm" action="#" method="post">
		<input type="text" name="notice_id" value="${bean.data.notice_id }" />
		<input type="text" name="notice_gid" value="${bean.data.notice_gid}" />
		<input type="text" name="notice_seq" value="${bean.data.notice_seq}" />
		<input type="text" name="notice_rev" value="${bean.data.notice_rev}" />
		<input type="text" name="page" value="${bean.page}"/>
		<table border="1" cellpadding="1" cellspacing="1">
			<tr>
				<td>제목</td>
				<td>
					<input type="text" name="notice_title" value="[Re]${bean.data.notice_title }">
				</td>
			</tr>
			<tr>	
				<td>작성자</td>
				<td>
					<input type="text" name="m_id">
				</td>
			</tr>
			<tr>
				<td>암호</td>
				<td>
					<input type="text" name="notice_pw">
				</td>
			</tr>
			<tr>	
				<td>내용</td>
				<td>
					<textarea rows="5" cols="30" name="notice_content">[Re]${bean.data.notice_content }</textarea>
				</td>
			</tr>
			<tr>
				<td>첨부파일</td>
				<td><input type="test" name="notice_upfile" > </td>
			</tr>
			<tr>
				<td colspan="2">
					<a href="#" onclick="reply_go(); return false;">작성</a>
					<a href="list">목록</a>
				</td>
			</tr>
			
		</table>
	</form>
</body>
</html>